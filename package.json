{
  "name": "aladdin-ui",
  "version": "1.0.0",
  "description": "aladdin ui components",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "dev": "docz dev",
    "start": "npm run dev",
    "build:doc": "rimraf doc-site && docz build",
    "preview:doc": "docz serve"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/fabric": "^2.3.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "docz": "^2.3.1",
    "gatsby-plugin-less": "^4.3.0",
    "husky": "^4.3.6",
    "less": "^3.13.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prismjs": "^1.22.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-simple-code-editor": "^0.11.0",
    "react-tooltip": "^4.2.11",
    "react-use": "^15.3.4",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "lint-staged": {
    "components/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "components/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
